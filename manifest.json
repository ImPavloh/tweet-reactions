{
  "manifest_version": 3,
  "name": "Tweet Reactions",
  "version": "1.0",
  "description": "Add Miiverse reactions to Twitter posts.",
  "author": "@impavloh",
  "icons": {
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*", "*://*.x.com/*"],
      "js": ["images.js", "content.js"],
      "css": ["style.css"]
    }
  ]
}
